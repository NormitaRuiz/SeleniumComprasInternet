<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite Luma Ecommerce">



    <test name="RegisterTestDebilPassword">
        <classes>
            <class name="nopCommerce.tests.RegisterTestDebilPassword">
                <methods>
                    <include name="registerNewAccountDebilPassword"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="LoginIncorrectPassword">
        <classes>
            <class name="nopCommerce.tests.LoginIncorrectPassword">
                <methods>
                    <include name="incorrectPasswordLogin"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="LoginUserEmailNoRegister">
        <classes>
            <class name="nopCommerce.tests.LoginUserEmailNoRegister">
                <methods>
                    <include name="userEmailNoRegister"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="LoginForgotPassword">
        <classes>
            <class name="nopCommerce.tests.LoginForgotPassword">
                <methods>
                    <include name="forgotEmailPassword"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="NewAddressFakerData">
        <classes>
            <class name="nopCommerce.tests.NewAddressFakerData">
                <methods>
                    <include name="newAddressFakeData"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="NewAddressShipIncompleteField">
        <classes>
            <class name="nopCommerce.tests.NewAddressShipIncompleteField">
                <methods>
                    <include name="newAddressIncompleteField"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="NewAddressFakerData">
        <classes>
            <class name="nopCommerce.tests.NewAddressFakerData">
                <methods>
                    <include name="newAddressFakeData"/>
                </methods>
            </class>
        </classes>
    </test>


</suite>