<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite Luma Ecommerce">


    <test name="RegisterTest">
        <classes>
            <class name="nopCommerce.tests.RegisterTest">
                <methods>
                    <include name="registerNewAccountExitFakeData"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="RegisterTestManualData">
        <classes>
            <class name="nopCommerce.tests.RegisterTestManualData">
                <methods>
                    <include name="registerNewAccountManual"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="RegisterTestIncompleteFields">
        <classes>
            <class name="nopCommerce.tests.RegisterTestIncompleteFields">
                <methods>
                    <include name="registerNewAccountIncompleteFields"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="LoginIncorrectPassword">
        <classes>
            <class name="nopCommerce.tests.LoginIncorrectPassword">
                <methods>
                    <include name="incorrectPasswordLogin"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="NewAddressFakerData">
        <classes>
            <class name="nopCommerce.tests.NewAddressFakerData">
                <methods>
                    <include name="newAddressFakeData"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="NewAddressManualData">
        <classes>
            <class name="nopCommerce.tests.NewAddressManualData">
                <methods>
                    <include name="newAddressManualData"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="BuySuccessful">
        <classes>
            <class name="nopCommerce.tests.BuySuccessful">
                <methods>
                    <include name="buyExit"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="BuySuccessCredCardInvalid">
        <classes>
            <class name="nopCommerce.tests.BuySuccessCredCardInvalid">
                <methods>
                    <include name="buyExit"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="buyCredCardInvalid">
        <classes>
            <class name="nopCommerce.tests.BuySuccessCredCardInvalid">
                <methods>
                    <include name="buyCredCardInvalid"/>
                </methods>
            </class>
        </classes>
    </test>



</suite>